{
  "name": "ba-tester",
  "version": "1.0.0",
  "author": "Basti√°n Gabriel Paredes Padget",
  "scripts": {
    "build": "webpack && next build",
    "dev": "npm run seed && next dev --turbopack",
    "dev:docker-compose": "docker compose up -d && lazydocker",
    "seed": "tsx -r dotenv/config ./src/libs/init/index.ts",
    "lint": "npx @biomejs/biome check",
    "lint:fix": "npx @biomejs/biome check --write",
    "start": "npm run seed && next start",
    "postinstall": "webpack",
    "test": "jest --ci --passWithNoTests",
    "test:u": "jest --ci --u",
    "generate-migration": "drizzle-kit generate --dialect=postgresql --schema=./src/libs/db/postgres/schema.ts --out=./src/libs/db/postgres/migrations"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@monaco-editor/react": "^4.7.0",
    "@mui/material": "^7.3.7",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "envalid": "^8.1.1",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.562.0",
    "magic-regexp": "^0.10.0",
    "monaco-editor": "^0.55.1",
    "mongoose": "^9.1.2",
    "next": "16.1.1",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.70.0",
    "react-tabs": "^6.1.0",
    "redis": "^5.10.0",
    "tsx": "^4.21.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "zod": "^4.3.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.8",
    "tailwindcss": "^4",
    "ts-loader": "^9.5.4",
    "typescript": "^5"
  }
}
